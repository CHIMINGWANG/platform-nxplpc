{
  "build": {
    "cpu": "cortex-m3",
    "f_cpu": "96000000L",
    "mcu": "lpc1768"
  },
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f", "scripts/interface/cmsis-dap.cfg",
            "-f", "scripts/target/lpc17xx.cfg"
          ]
        },
        "gdbinit": [
          "target remote :3333",
          "file $PROG_PATH",
          "load $PROG_PATH",
          "monitor init",
          "monitor reset halt",
          "tb main"
        ],
        "onboard": true
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Seeed Arch Pro",
  "upload": {
    "maximum_ram_size": 65536,
    "maximum_size": 524288
  },
  "url": "https://developer.mbed.org/platforms/Seeeduino-Arch-Pro/",
  "vendor": "SeeedStudio"
}